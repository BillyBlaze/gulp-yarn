{
  "name": "gulp-yarn",
  "version": "0.1.0",
  "description": "Automatically install yarn, packages/dependencies.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test istanbul cover _mocha -R spec test/**/*.js ",
    "lint": "xo index.js",
    "lint:fix": "xo index.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/warapitiya/gulp-yarn"
  },
  "keywords": [
    "gulpplugin",
    "yarn",
    "yarnpkg",
    "gulp",
    "install"
  ],
  "author": "Malindu Warapitiya <warapitiya93@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/warapitiya/gulp-yarn/issues"
  },
  "homepage": "https://github.com/warapitiya/gulp-yarn",
  "dependencies": {
    "gulp-util": "^3.0.4",
    "through2": "^2.0.0",
    "which": "^1.0.9"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.16",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "xo": "^0.17.1"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "globals": [],
    "rules": {
      "indent": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    }
  }
}
