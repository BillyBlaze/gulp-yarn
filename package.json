{
  "name": "gulp-yarn",
  "version": "1.0.0-alpha.1",
  "description": "Automatically install yarn, packages/dependencies.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run build && NODE_ENV=test istanbul cover _mocha -R test/**/*.js -- --compilers js:babel-register",
    "lint": "xo src/**/*.js",
    "lint:fix": "xo src/**/*.js --fix",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/warapitiya/gulp-yarn"
  },
  "keywords": [
    "gulpplugin",
    "yarn",
    "yarnpkg",
    "gulp",
    "install"
  ],
  "author": "Malindu Warapitiya <warapitiya93@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/warapitiya/gulp-yarn/issues"
  },
  "homepage": "https://github.com/warapitiya/gulp-yarn",
  "dependencies": {
    "gulp-util": "^3.0.4",
    "through2": "^2.0.0",
    "which": "^1.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "chai": "^3.2.0",
    "coveralls": "^2.11.16",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "xo": "^0.17.1"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "globals": [],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    }
  }
}
